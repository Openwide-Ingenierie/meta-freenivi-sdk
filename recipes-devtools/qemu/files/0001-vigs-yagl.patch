+
+    void (*context_destroy)(struct yagl_egl_driver */*driver*/,
+                            EGLNativeDisplayType /*dpy*/,
+                            EGLContext /*ctx*/);
+
+    bool (*make_current)(struct yagl_egl_driver */*driver*/,
+                         EGLNativeDisplayType /*dpy*/,
+                         EGLSurface /*draw*/,
+                         EGLSurface /*read*/,
+                         EGLContext /*ctx*/);
+
+    void (*destroy)(struct yagl_egl_driver */*driver*/);
