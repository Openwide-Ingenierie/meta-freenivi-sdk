+
+    YAGL_LOG_FUNC_ENTER(yagl_egl_onscreen_image_destroy, "%u", obj->global_name);
+
+    image->ws_sfc->base.release(&image->ws_sfc->base);
+
+    g_free(image);
+
+    YAGL_LOG_FUNC_EXIT(NULL);
+}
