+                             0, &pbuffer);
+
+    if (error) {
+        YAGL_LOG_ERROR("CGLCreatePBuffer failed: %s", CGLErrorString(error));
+        pbuffer = NULL;
+    }
+
+    YAGL_LOG_FUNC_EXIT("%p", pbuffer);
