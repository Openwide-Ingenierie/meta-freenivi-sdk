+static void yagl_gles_api_destroy(struct yagl_api *api)
+{
+    struct yagl_gles_api *gles_api = (struct yagl_gles_api*)api;
+
+    gles_api->driver->destroy(gles_api->driver);
+    gles_api->driver = NULL;
