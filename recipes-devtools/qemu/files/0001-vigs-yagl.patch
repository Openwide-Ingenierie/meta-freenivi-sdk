+struct vigs_backend *vigs_gl_backend_create(void *display)
+{
+    WNDCLASSEXA vigs_win_class;
+    HWND tmp_win = NULL;
+    HDC tmp_dc = NULL;
+    HGLRC tmp_ctx = NULL;
+    int config_id = 0;
+    PIXELFORMATDESCRIPTOR tmp_pixfmt = {
+        .nSize = sizeof(PIXELFORMATDESCRIPTOR),
