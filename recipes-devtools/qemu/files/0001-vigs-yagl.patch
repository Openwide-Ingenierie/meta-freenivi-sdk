+#define VIGS_LOG_ERROR(format, ...) VIGS_LOG_EVENT(error, format,##__VA_ARGS__)
+#define VIGS_LOG_CRITICAL(format, ...) vigs_log_event(vigs_log_level_error, __FUNCTION__, __LINE__, format,##__VA_ARGS__)
+
+#endif
