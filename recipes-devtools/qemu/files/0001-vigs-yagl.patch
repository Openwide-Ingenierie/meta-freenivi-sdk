+void vigs_surface_init(struct vigs_surface *sfc,
+                       struct winsys_surface *ws_sfc,
+                       struct vigs_backend *backend,
+                       uint32_t stride,
+                       vigsp_surface_format format,
+                       vigsp_surface_id id)
