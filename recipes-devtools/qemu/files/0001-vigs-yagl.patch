+    struct yagl_vector v;
+
+    union
+    {
+        target_ulong va;
+        uint32_t offset;
+    };
+
+    int32_t el_size;
+    int32_t *count;
+};
+
+struct yagl_transport
+{
+    /*
+     * per-transport.
+     * @{
+     */
+
+    uint8_t **pages;
+
+    /*
+     * @}
+     */
+
