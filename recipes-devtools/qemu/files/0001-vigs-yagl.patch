+static struct vigs_winsys_gl_surface
+    *vigs_winsys_gl_surface_create(struct vigs_gl_backend *backend,
+                                   struct vigs_gl_surface *parent,
+                                   uint32_t width,
+                                   uint32_t height,
+                                   GLint tex_internalformat,
+                                   GLenum tex_format,
+                                   GLenum tex_type)
+{
+    struct vigs_winsys_gl_surface *ws_sfc;
